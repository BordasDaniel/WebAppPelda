@{
    ViewData["Title"] = "Ügyfelek táblázatban";
}

<div class="text-center">
    <h1 action="display-4">Ügyfelek adatai táblázatban</h1>
    <table>
        <tr>
            <th>Név</th>
            <th>Telefonszám</th>
            <th>Pontszám</th>
        </tr>
        @foreach(Customer customer in Model)
         {
        <tr>
            <td>@(customer.Name)</td>
            <td>@(customer.Phone)</td>
            <td>@(customer.Score)</td>
        </tr>
        }
    </table>
    
</div>
